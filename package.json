		"chalk": "^2.3.0",
		"indent-string": "^3.2.0",
		"nodemon": "^1.14.3",
		"source-map-loader": "^0.2.3",
		"ts-node": "^4.1.0",
		"typescript": "^2.6.2",
		"webpack": "^3.10.0"
	}
